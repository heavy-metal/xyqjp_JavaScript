/* pages/login/coachLogin/checkStudentDetail/index.wxss */
.detail_list_wrap {
  display: flex;
  justify-content: space-around;
  border-bottom: 2rpx solid var(--lineColor);
  padding: 24rpx 10rpx;
}
.detail_list_wrap .detail_title {
  flex: 2;
  display: flex;
  align-items: center;
}
.detail_list_wrap .detail_title .title {
  font-size: 30rpx;
}
.detail_list_wrap .detail_subtitle {
  flex: 6;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.detail_list_wrap .detail_subtitle .subtitle {
  color: darkgray;
  text-align: right;
}
.student_info {
  display: flex;
  align-items: center;
  border-bottom: 5rpx solid var(--lineColor);
  height: 120rpx;
  padding: 20rpx;
}
.student_info .left_info_wrap {
  flex: 1;
}
.student_info .left_info_wrap .info_wrap {
  display: flex;
}
.student_info .left_info_wrap .info_wrap .name {
  padding: 0 10rpx;
}
.title_tab {
  border-bottom: 5rpx solid var(--lineColor);
  padding-bottom: 30rpx;
}
.title_tab .title_contain {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx;
}
.title_tab .title_contain .left_title_wrap {
  display: flex;
}
.title_tab .title_contain .left_title_wrap .states {
  color: darkgray;
}
.title_tab .progress_contain {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  text-align: center;
}
