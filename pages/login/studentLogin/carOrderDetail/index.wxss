/* pages/login/studentLogin/carOrderDetail/index.wxss */
.detail_list_wrap {
  display: flex;
  justify-content: space-around;
  border-bottom: 2rpx solid var(--lineColor);
  padding: 20rpx 10rpx;
}
.detail_list_wrap .detail_title {
  flex: 1;
  display: flex;
  align-items: center;
}
.detail_list_wrap .detail_title .title {
  font-size: 30rpx;
}
.detail_list_wrap .detail_subtitle {
  flex: 4;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.detail_list_wrap .detail_subtitle .subtitle {
  color: darkgray;
  text-align: right;
}
.detail_applyBtn_wrap {
  padding-top: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.detail_applyBtn_wrap .detail_applyBtn {
  width: 80vw;
  border: 1rpx solid darkgray;
  border-radius: 30rpx;
  padding: 25rpx 10rpx;
  text-align: center;
}
.score_wrap .score_title_contain {
  display: flex;
  align-items: center;
  padding: 25rpx 20rpx;
}
